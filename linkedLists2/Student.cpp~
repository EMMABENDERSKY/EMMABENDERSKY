#include <iostream>
#include <cstring>
#include "Student.h"

using namespace std;

Student::Student(char* newFirstName, char* newLastName, int newID, double newGPA)
{
  firstName = new char[80];
  strcpy(firstName, newFirstName);
  lastName = new char[80];
  strcpy(lastName, newlastName);
  ID = newID;
  GPA = newGPA;
}
