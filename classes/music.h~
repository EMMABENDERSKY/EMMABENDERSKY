#include <iostream>
#include <cstring>
#ifndef MUSIC_H
#define MUSIC_H
#include "media.h"

using namespace std;

class music : public media
{
 private:
  char* artist;
  double duration;
  char* publisher;
 public:
  music();
  music(char*, double, char*);
  char* getArtist();
  double getDuration();
  char* getPublisher();
};
#endif
